{
  "appenders": [
    {
      "type": "console"
    },
    {
      "type": "dateFile",
      "filename": "./logs/koa-redis",
      "pattern": "-yyyy-MM-dd.log",
      "alwaysIncludePattern": true
    },
    {
      "type": "logLevelFilter",
      "level": "ERROR",
      "appender": {
        "type": "file",
        "filename": "./logs/errors.log",
        "pattern": "-yyyy-MM-dd.log",
        "alwaysIncludePattern": true
      }
    }
  ]
}